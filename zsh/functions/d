# Quickly change to frequently used directories

local -A aliases
aliases=(
    d        $PROJECTS/
    g        $PROJECTS/gap/gid/
    gs       $PROJECTS/gap/svn/
    bb       $PROJECTS/gap/brazil-build/
    bd       $BRAINDUMP
    bin      $USR/bin/
    dotfiles $DOTFILES
)
aliases[df]=$aliases[dotfiles]

cd ${aliases[${1:-d}]}
pwd
