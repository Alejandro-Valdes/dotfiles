# Quickly change to frequently used directories

local -A aliases
aliases=(
    d        "$PROJECTS_DIR/sg"
    g        "$PROJECTS_DIR/gma/"
    oc       "$PROJECTS_DIR/sg/opencash/"
    b        "$PROJECTS_DIR/sg/sgeb.me/"
    tk       "$PROJECTS_DIR/sg/go-tuikit"
    go       "$PROJECTS_DIR/go"
    bd       "$BRAINDUMP_DIR"
    bin      "$USR_DIR/bin/"
    dotfiles "$DOTFILES_DIR"
    df       "$DOTFILES_DIR"
)

cd ${aliases[${1:-d}]}
pwd
