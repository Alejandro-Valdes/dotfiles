# Quickly change to frequently used directories

local -A aliases
aliases=(
    d        "$PROJECTS_DIR/"
    g        "$PROJECTS_DIR/gp/svn/"
    gd       "$PROJECTS_DIR/gp/d2c/"
    bb       "$PROJECTS_DIR/gp/bb/"
    oc       "$PROJECTS_DIR/sg/opencash/"
    b        "$PROJECTS_DIR/sg/sgeb.me/"
    bd       "${BRAINDUMP_DIR}"
    bin      "$USR_DIR/bin/"
    dotfiles "$DOTFILES_DIR"
)
aliases[df]=$aliases[dotfiles]

cd ${aliases[${1:-d}]}
pwd
