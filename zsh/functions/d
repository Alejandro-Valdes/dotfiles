# Quickly change to frequently used directories

local -A aliases
aliases=(
    d        $PROJECTS/
    g        $PROJECTS/gp/svn/
    gd       $PROJECTS/gp/d2c/
    bb       $PROJECTS/gp/bb/
    oc       $PROJECTS/sg/opencash/
    bd       $BRAINDUMP
    b        $PROJECTS/sg/sgeb.me/
    bin      $USR/bin/
    dotfiles $DOTFILES
)
aliases[df]=$aliases[dotfiles]

cd ${aliases[${1:-d}]}
pwd
